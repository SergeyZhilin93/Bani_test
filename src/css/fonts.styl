@font-face
    font-family 'Museo Sans Cyrl'
    src url('../assets/fonts/Museo/MuseoSansCyrl_1.otf')
    font-weight 400
    font-style normal

@font-face
    font-family 'Museo Sans Cyrl Light'
    src url('../assets/fonts/Museo/MuseoSansCyrl_0.otf')
    font-weight 400
    font-style normal

@font-face
    font-family 'Museo Sans Cyrl Bold'
    src url('../assets/fonts/Museo/MuseoSansCyrl_3.otf')
    font-weight 900
    font-style normal

@font-face
    font-family 'Museo Cyrl'
    src url('../assets/fonts/MuseoCyrl500.otf')
    font-weight 500
    font-style normal

@font-face
    font-family 'Lato'
    src url('../assets/fonts/Lato-Regular.ttf')
    font-weight normal
    font-style normal

@font-face
    font-family 'Museo Sans Cyrl'
    src url('../assets/fonts/MuseoSansCyrl-300.eot')
    src url('../assets/fonts/MuseoSansCyrl-300.eot?#iefix') format('embedded-opentype'),
        url('../assets/fonts/MuseoSansCyrl-300.woff2') format('woff2'),
        url('../assets/fonts/MuseoSansCyrl-300.woff') format('woff'),
        url('../assets/fonts/MuseoSansCyrl-300.ttf') format('truetype')
    font-weight 300
    font-style normal
